or(advisor){\n      let id = advisor.advisorID\n      if(confirm(\"Do you really want to delete?\")){\n    courseServices.deleteAdvisor(id)\n      .then(() => {\n         this.$router.push({ name: 'advisorlist' }) \n        })\n       \n        \n        .catch(error => {\n          console.log(error)\n        })\n      }\n    },\n    \n}\n}\n</script>\n\n<style>\nth {\n  text-align: left;\n  font-size: 1.5rem !important;\n}\n</style> "]}]}