\"advisor.dept\" type=\"text\" id=\"advisorLevel\"/>\n       </v-col>\n    </v-form>\n<confirm-dialog ref=\"confirmDialog\"></confirm-dialog>\n\n  </div>\n</template>\n\n<script>\nimport courseServices from '@/services/courseServices.js'\nimport ConfirmDialog from '@/components/ConfirmDialog.vue'\n\nexport default {\n  props: ['id'],\n  components: {ConfirmDialog},\n  data() {\n    return {\n      advisor: {}\n      \n    }\n  },\n  created() {\n      courseServices.getAdvisor(this.id)\n      .then(response => {\n        this.advisor = response.data,\n        console.log(this.id)\n      })\n      .catch(error => {\n        console.log('There was an error:', error.response)\n        \n      })\n        \n      .catch(error => {\n        console.log('There was an error:', error.response)\n      })\n  },\n  methods: {\n    addForm(){\n      this.addAdvisorDisplay = true;\n    },\n    updateAdvisor(advisor) {\n          this.$router.push({ name: 'editadvisor', params: {id: advisor.advisorID}})\n        .then(() => {\n        })\n        .catch(error => {\n         console.log(error)\n        })\n    },\n    cancel() {\n      this.$router.push({ name: 'advisorlist' })\n    },\n\n     async deleteAdvisor(advisor){\n      let id = advisor.advisorID\n      if(confirm(\"Do you really want to delete?\")){\n    courseServices.deleteAdvisor(id)\n      .then(() => {\n         this.$router.push({ name: 'advisorlist' }) \n        })\n       \n        \n        .catch(error => {\n          console.log(error)\n        })\n      }\n    },\n    \n}\n}\n</script>\n\n<style>\nth {\n  text-align: left;\n  font-size: 1.5rem !important;\n}\n</style> "],"sourceRoot":"src/views/Advisors"}]}